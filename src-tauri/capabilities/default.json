{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-execute",
    "dialog:allow-confirm",
    "dialog:allow-open",
    "fs:allow-app-read",
    "fs:allow-read-dir",
    "fs:allow-app-read-recursive",
    "fs:allow-open"
  ]
}
